{
    "name": "javleds/meetup-api-client",
    "description": "PHP API wrapper for meetup.com",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Javier Ledezma",
            "email": "juls0593@gmail.com"
        }
    ],
    "scripts": {
        "phpunit": "phpunit",
        "phpcs": "php-cs-fixer fix -v --diff --dry-run",
        "phpcs-fix": "php-cs-fixer fix -v --diff"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Javleds\\MeetupApiClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Javleds\\MeetupApiClient\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "8.5.x-dev",
        "friendsofphp/php-cs-fixer": "^2.17@dev"
    }
}
